{
    "version": 2,
    "buildCommand": "node build.js",
    "devCommand": "next dev",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 